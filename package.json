{
  "name": "writings",
  "version": "1.0.0",
  "scripts": {
    "prettier": "prettier --write \"src/**/*.{ts,json}\"",
    "d": "rimraf data/downloaded && ts-node src/download && prettier --write \"data/downloaded/**/*.json\"",
    "p": "rimraf data/parsed && ts-node src/parse && prettier --write \"data/parsed/**/*.json\"",
    "e": "rimraf data/extracted && rimraf data/extracted.json && ts-node src/extract && prettier --write \"data/extracted**/*.json\" && prettier --write \"data/extracted.json\"",
    "c": "rimraf data/data.json && ts-node src/clean && prettier --write \"data/data.json\""
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "devDependencies": {
    "@types/node": "^13.7.1",
    "fs-extra": "^8.1.0",
    "js-levenshtein": "^1.1.6",
    "maraca-app": "^0.2.6",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "pdf2json": "^1.1.8",
    "prettier": "^1.19.1",
    "prettier-plugin-maraca": "^0.3.4",
    "rehype-parse": "^6.0.2",
    "rimraf": "^3.0.2",
    "roman-numbers": "^1.0.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "unified": "^8.4.2",
    "webfontloader": "^1.6.28"
  }
}
